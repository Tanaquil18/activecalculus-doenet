  <boolean name="B2correct">$B2.creditAchieved=1</boolean>
  <boolean name="B3correct">$B3.creditAchieved=1</boolean>
  <boolean name="B4correct">$B4.creditAchieved=1</boolean>
  <boolean name="C1correct">$C1.creditAchieved=1</boolean>
  <boolean name="C2correct">$C2.creditAchieved=1</boolean>
  <boolean name="C3correct">$C3.creditAchieved=1</boolean>
  <boolean name="C4correct">$C4.creditAchieved=1</boolean>
  <boolean name="D1correct">$D1.creditAchieved=1</boolean>
  <boolean name="D2correct">$D2.creditAchieved=1</boolean>  
