<exploration xmlns:xi="http://www.w3.org/2001/XInclude" xml:id="PA-7-6-D" label="PA-7-6-doenet" component="doenet">

    <figure xml:id="F-PA-7-6-doenet">
        <caption>At left, the slope field for <m>\frac{dP}{dt} = \frac{1}{2}P</m>; at right, the slope field for <m>\frac{dP}{dt} = \frac{1}{2}P(3-P)</m></caption>
        <sidebyside widths="40% 40%">
         <image width="25%">
          <prefigure label="Fig-PA-7-6-doenet-1a" xmlns="https://prefigure.org">
            <diagram dimensions="(300,300)" margins="10">
            <definition>f(t,y) = 0.5*y</definition>
                <coordinates bbox="[-0.5,-0.5,4,4]">
                <grid-axes xlabel="t" ylabel="y"/>
                <slope-field function="f"/>
                </coordinates>
            </diagram>
          </prefigure>
        </image>
        <image width="25%">
          <prefigure label="Fig-PA-7-6-doenet-1b" xmlns="https://prefigure.org">
            <diagram dimensions="(300,300)" margins="10">
            <definition>f(t,y) = 0.5*y*(3-y)</definition>
                <coordinates bbox="[-0.5,-0.5,4,4]">
                <grid-axes xlabel="t" ylabel="y"/>
                <slope-field function="f"/>
                </coordinates>
            </diagram>

          </prefigure>
        </image>           
        </sidebyside>
     </figure>  

  <interactive xml:id="interactive-PA-7-6-D" platform="doenetml" width="100%" aspect="2:8">
    <slate surface="doenetml">
      <xi:include href="doenet/PA-7-6-D.doenetml" parse="text"/> 
    </slate>  
  </interactive>

</exploration>
