                    <when>(<mod>$j 4</mod> = 1 and $gjderik = $g1P) or (<mod>$j 4</mod> = 2 and $gjderiv = $g2P) or (<mod>$j 4</mod> = 3 and $gjderiv = $g3P) or (<mod>$j 4</mod> = 0 and $gjderiv = $g</when>
