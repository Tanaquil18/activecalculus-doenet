            <cell>
              <m>g''(x) = </m><answer><award>$g2P</award></answer>
            </cell>    
          </row>  
          <row>
            <cell>
              <m>g'''(x) = </m><answer><award>$g3P</award></answer>
            </cell> 
            <cell>
              <m>g^{(4)}(x) = </m><answer><award>$g</award></answer>
            </cell>     
            <cell>
              <m>g^{(5)}(x) = </m><answer><award>$g1P</award></answer>
            </cell>    
          </row>  
          <row> 
            <cell>
              <m>g^{(6)}(x) = </m><answer><award>$g2P</award></answer>
