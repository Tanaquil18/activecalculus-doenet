  <setup>
    <math name="a"><selectFromSequence from="-1.5" to="2.5" step="0.5"/></math>
    <math name="b" simplify>$a+0.5</math>
    <math name="c" simplify>$a+1</math>    
    <function name="f" symbolic="false" domain="[-2,4]" styleNumber="1">
      <label><m>f</m></label>
      2*cos(pi*(x-1))+0.5
    </function> 
    <derivative name="fP">$f</derivative>
    <derivative name="fPP">$fP</derivative>

    <math name="signfa"><sign>$$f($a)</sign></math>
    <math name="signfPa"><sign>$$fP($a)</sign></math>
    <math name="signfPPa"><sign>$$fPP($a)</sign></math>
    <math name="signfb"><sign>$$f($b)</sign></math>
    <math name="signfPb"><sign>$$fP($b)</sign></math>
    <math name="signfPPb"><sign>$$fPP($b)</sign></math>
    <math name="signfc"><sign>$$f($c)</sign></math>
    <math name="signfPc"><sign>$$fP($c)</sign></math>
    <math name="signfPPc"><sign>$$fPP($c)</sign></math>
    
    <function name="g" symbolic="false" domain="[-2,4]" styleNumber="2">
      <label><m>g</m></label>
      -1*cos(pi/2*(x-2))+0.5
    </function>   
    <derivative name="gP">$g</derivative>
    <derivative name="gPP">$gP</derivative> 

    <math name="signga"><sign>$$g($a)</sign></math>
    <math name="signgPa"><sign>$$gP($a)</sign></math>
    <math name="signgPPa"><sign>$$gPP($a)</sign></math>  
    <math name="signgb"><sign>$$g($b)</sign></math>
    <math name="signgPb"><sign>$$gP($b)</sign></math>
    <math name="signgPPb"><sign>$$gPP($b)</sign></math>  
    <math name="signgc"><sign>$$g($c)</sign></math>
    <math name="signgPc"><sign>$$gP($c)</sign></math>
    <math name="signgPPc"><sign>$$gPP($c)</sign></math>  
    
  </setup>  
