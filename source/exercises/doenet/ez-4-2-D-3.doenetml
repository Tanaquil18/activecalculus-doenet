  <rectangle fixed name="rt1" vertices="($s1,0) ($s1+0.5,$$f2($s1))" styleNumber="3" filled/>
  <rectangle fixed name="rt2" vertices="($s1+0.5,0) ($s1+1,$$f2($s1+0.5))" styleNumber="3" filled/>
  <rectangle fixed name="rt3" vertices="($s1+1,0) ($s1+1.5,$$f2($s1+1))" styleNumber="3" filled/>
  <rectangle fixed name="rt4" vertices="($s1+1.5,0) ($s1+2,$$f2($s1+1.5))" styleNumber="3" filled/>
